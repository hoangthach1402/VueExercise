{
  "name": "my-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "electron:dev": "concurrently \"npm run dev\" \"cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron --enable-experimental-vm-modules .\"",
    "build:web": "vue-tsc -b && vite build",
    "build:electron": "npm run build:web && npm run build:main && electron-builder",
    "build:main": "node esbuild-main.js",
    "preview": "vite preview",
    "serve-db": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@google-cloud/translate": "^9.0.1",
    "@ownego/polaris-vue": "^2.1.26",
    "@tailwindcss/vite": "^4.1.4",
    "antd": "^5.24.5",
    "axios": "^1.8.4",
    "chart.js": "^4.4.9",
    "esbuild": "^0.25.3",
    "pinia": "^3.0.1",
    "sass": "^1.86.1",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.0",
    "electron-builder": "^24.13.3",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  },
  "build": {
    "appId": "com.your.app",
    "files": [
      "dist/**/*",
      "electron/preload.js",
      "electron/main.js"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
